<?php

/**
 * @file
 * Contains number_to_word.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function number_to_word_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the number_to_word module.
    case 'help.page.number_to_word':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Module to convert Numbers to words.') . '</p>';
      return $output;

    default:
  }
}
